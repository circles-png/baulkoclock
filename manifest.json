{
    "manifest_version": 2,
    "name": "Baulko Clock",
    "description": "A bell times extension for Baulkham Hills High School.",
    "version": "1.2.1",
    "icons": {
         "128": "Images/Orange.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "Images/Orange.png"
    },
    "permissions": ["storage"],
    "applications": {
        "gecko": {
            "id": "baulko@clock"
        }
    }
}
